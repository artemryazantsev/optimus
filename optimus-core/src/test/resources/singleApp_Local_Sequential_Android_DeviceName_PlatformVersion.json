{
  "executionDetails": {
    "appium_js_path": "/usr/local/bin/appium",
    "appium_node_path": "/usr/local/bin/node"
  },
  "testFeed": [
    {
      "belongsTo": "rider",
      "runsOn": "emulator",
      "appDir": "app",
      "optimusDesiredCapabilities": {
        "appiumServerCapabilities": {
          "app": "app-staging-release.apk",
          "platformName": "Android",
          "deviceName": "Redmi",
          "platformVersion": "6.0",
          "newCommandTimeout": 180
        },
        "androidOnlyCapabilities": {
          "appActivity": "com.tvapp.app.Splash",
          "appPackage": "com.tvapp.app.staging",
          "avdLaunchTimeout": 300000,
          "useKeystore": false
        }
      },
      "deviceState": {
        "clearNotification": true,
        "clearADBLogs": true,
        "grantAllPermissions": true,
        "captureVideo": true
      }
    }
  ]
}